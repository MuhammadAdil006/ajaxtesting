@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<form id="fromdata" enctype="multipart/form-data" method="post">
  <div class="form-group">
    <label for="email">Email address</label>
    <input type="email" class="form-control" id="email" placeholder="name@example.com">
  </div>
  <div class="form-group">
    <label for="exampleFormControlInput2">id</label>
    <input type="number" class="form-control" id="ID" placeholder="e.g:23">
  </div>
 
  <div class="form-group">
    <label for="message">Example textarea</label>
    <textarea class="form-control" id="message" rows="3"></textarea>
  </div>
    <div class="form-group">
    <label for="image">Example file input</label>
     <input id="files" class="form-control-file" accept="image/*" name="files" type="file"  />
     </div>
  <button id="btnSubmit" type="button">Submit</button>
</form>